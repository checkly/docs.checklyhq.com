{
  "name": "checklyhq.com",
  "scope": "checkly",
  "build": {
    "env": {}
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-frame-options",
          "value": "deny"
        },
        {
          "key": "x-xss-protection",
          "value": "0"
        },
        {
          "key": "x-content-type-options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/(.*).(bmp|ico|icns|jpeg|pct|png|tiff|webp|svg|woff|woff2)",
      "headers": [
        {
          "key": "cache-control",
          "value": "max-age=604800, s-maxage=604800"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "docs.checklyhq.com"
        }
      ],
      "destination": "https://www.checklyhq.com/docs/"
    },
    { "source": "/learn/headless/e2e-coupon/", "destination": "/learn/playwright/e2e/checkout/", "permanent": true }
  ]
}
